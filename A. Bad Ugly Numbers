#include <bits/stdc++.h>
using namespace std;

void check_num() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        if (n == 1) {
            cout << "-1" << endl;
        } else {
        cout << 2;
            for(int i=1;i<n;i++){
            cout <<3;
            }
            cout << endl;
       }
    }
}

int main() {
    check_num();
    return 0;
}
